<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.itcast.entity.Orders" table="orders">
		<id name="oid" column="oid">
			<generator class="native"></generator>
		</id>
		<property name="orderCode" column="orderCode"></property>
		<property name="orderSum" column="orderSum"></property>
		<property name="orderDate" column="orderDate"></property>
		<property name="orderName" column="orderName"></property>
		
	</class>
	
</hibernate-mapping>
